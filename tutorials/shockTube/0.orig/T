FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1;

boundaryField
{
    side1
    {
        type            zeroGradient;
    }
    side2
    {
        type            zeroGradient;
    }
    wall
    {
        type            zeroGradient;
    }
    empty
    {
        type            empty;
    }
}

