FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    side1
    {
        type            zeroGradient;
    }

    side2
    {
        type            zeroGradient;
    }
    wall
    {
        type            slip;
    }
    empty
    {
        type            empty;
    }
}

